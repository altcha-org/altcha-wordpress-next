{
  "compilerOptions": {
    "target": "ES2017",
    "module": "ESNext",
    "lib": ["DOM", "ES2017"],
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": false,
    "sourceMap": false
  },
  "include": ["src/integrations/awsmteam.ts", "src/integrations/awsmteam-admin.ts"],
  "exclude": ["node_modules", "public", "dist", "src/**/*.test.ts"]
}